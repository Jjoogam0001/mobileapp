<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="WheelDurationInput">
        <attr name="fadedTextColor" format="color" />
    </declare-styleable>
        
    <declare-styleable name="TooltipLayout">
        <attr name="arrowDirection" format="enum">
          <enum name="bottomRight" value="0"/>
          <enum name="bottomLeft" value="1"/>
          <enum name="bottomCenter" value="2"/>
          <enum name="topLeft" value="3"/>
          <enum name="topCenter" value="4"/>
        </attr>
    </declare-styleable>
    
    <declare-styleable name="BarChartView">
        <attr name="drawEmptyState" format="boolean"/>
    </declare-styleable>
</resources>